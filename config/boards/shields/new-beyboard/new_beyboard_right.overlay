// new_beyboard_left.overlay

#include "new_beyboard.dtsi" // Notice that the main dtsi files are included in the overlay.

&default_transform { // The matrix transform for this board is 5 columns over because the left half is 5 columns wide according to the matrix.
    col-offset = <5>;
};

&kscan0 {
    col-gpios
        = <&pro_micro 18 GPIO_ACTIVE_HIGH> // col0 in the schematic
        , <&pro_micro 15 GPIO_ACTIVE_HIGH> // col1 in the schematic
        , <&pro_micro 14 GPIO_ACTIVE_HIGH> // col2 in the schematic
        , <&pro_micro 16 GPIO_ACTIVE_HIGH> // col3 in the schematic
        , <&pro_micro 10 GPIO_ACTIVE_HIGH> // col4 in the schematic
        ;
};